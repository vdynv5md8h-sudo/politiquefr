{
  "name": "@politiquefr/backend",
  "version": "1.0.0",
  "private": true,
  "description": "API backend pour PolitiqueFR",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon",
    "build": "prisma generate && tsc",
    "start": "node dist/app.js",
    "start:prod": "npm run db:migrate:prod && node dist/app.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "clean": "rm -rf dist",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma db push --skip-generate",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@mistralai/mistralai": "^1.10.0",
    "@prisma/client": "^5.6.0",
    "@types/adm-zip": "^0.5.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/sanitize-html": "^2.9.5",
    "adm-zip": "^0.5.16",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "fuse.js": "^7.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "prisma": "^5.6.0",
    "rss-parser": "^3.13.0",
    "sanitize-html": "^2.11.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "vitest": "^1.0.0"
  }
}
